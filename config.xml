        <splash height="768" platform="ios" src="www/res/ios/Default-Landscape.png" width="1024" />
        <splash height="2048" platform="ios" src="www/res/ios/Default-Portrait@2x.png" width="1536" />
        <splash height="1536" platform="ios" src="www/res/ios/Default-Landscape@2x.png" width="2048" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <allow-navigation href="*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <allow-navigation href="wss://*/*" />
    <allow-navigation href="data:*" />
    <engine name="ios" spec="~4.0.0" />
    <engine name="android" spec="~6.0.0" />
    <plugin name="cordova-plugin-device" />
    <plugin name="cordova-plugin-network-information" />
    <plugin name="cordova-plugin-splashscreen" />
    <plugin name="cordova-plugin-geolocation" />
    <plugin name="cordova-plugin-inappbrowser" />
    <plugin name="cordova-plugin-camera" />
    <plugin name="cordova-plugin-file-transfer" />
    <plugin name="cordova-plugin-media-capture" spec="*">
        <variable name="CAMERA_USAGE_DESCRIPTION" value="Subir Expediente" />
        <variable name="MICROPHONE_USAGE_DESCRIPTION" value="Subir Expediente" />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value="Subir Expediente" />
