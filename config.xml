<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="302" id="com.nucleo.cliomedic" version="1.3.5" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Cliomedic</name>
    <description>
       Cliomedic
    </description>
    <author email="dev@n4ymedio.mx" href="n4ymedio.mx">
        Nucleo 4 y Medio
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-installLocation" value="auto" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="target-device" value="handset" />
    <preference name="BackgroundColor" value="0x0000" />
    <preference name="stay-in-webview" value="false" />
    <preference name="ios-statusbarstyle" value="black-opaque" />
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.GeoBroker" />
    </feature>
    <feature name="Geolocation">
        <param name="ios-package" value="CDVLocation" />
    </feature>
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <feature name="http://api.phonegap.com/1.0/geolocation" />
    <feature name="http://api.phonegap.com/1.0/device" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenBackgroundColor" value="0xffff" />
    <preference name="SplashShowOnlyFirstTime" value="true" />
    <splash src="splash.png" />
    <icon src="icon.png" />
   
    <access origin="*" subdomains="true" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <preference name="EnableViewportScale" value="false" />
    <access launch-external="yes" origin="geo:*" />
    <access launch-external="yes" origin="google.navigation:*" />
    <access origin="*://*.googleapis.com/*" subdomains="true" />
    <access origin="*://*.gstatic.com/*" subdomains="true" />
    <access origin="*://*.google.com/*" subdomains="true" />
    <access origin="*://*.googleusercontent.com/*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
     <platform name="ios">

        <icon src="www/res/ios/Icon-App-20x20@1x.png" width="20" height="20" />
        <icon src="www/res/ios/Icon-App-20x20@2x.png" width="40" height="40" />
        <icon src="www/res/ios/Icon-App-20x20@3x.png" width="60" height="60" />
      
        <icon src="www/res/ios/Icon-App-29x29@1x.png" width="29" height="29" />
        <icon src="www/res/ios/Icon-App-29x29@2x.png" width="58" height="58" />
        <icon src="www/res/ios/Icon-App-29x29@3x.png" width="87" height="87" />

        <icon src="www/res/ios/Icon-App-40x40@1x.png" width="40" height="40" />
        <icon src="www/res/ios/Icon-App-40x40@2x.png" width="80" height="80" />
        <icon src="www/res/ios/Icon-App-40x40@3x.png" width="120" height="120" />

        <icon src="www/res/ios/Icon-App-60x60@1x.png" width="60" height="60" />
        <icon src="www/res/ios/Icon-App-60x60@2x.png" width="120" height="120" />
        <icon src="www/res/ios/Icon-App-60x60@3x.png" width="180" height="180" />
        
        <icon src="www/res/ios/Icon-App-76x76@1x.png" width="76" height="76" />
        <icon src="www/res/ios/Icon-App-76x76@2x.png" width="152" height="152" />
        

        <icon src="www/res/ios/Icon-App-83.5x83.5@2x.png" width="167" height="167" />
       
        <icon src="www/res/ios/ItunesArtwork@2x.png" width="1024" height="1024" />
        

        <splash height="480" platform="ios" src="www/res/ios/Default.png" width="320" />
        <splash height="960" platform="ios" src="www/res/ios/Default@2x.png" width="640" />
        <splash height="1136" platform="ios" src="www/res/ios/Default-568h@2x.png" width="640" />
        <splash height="1334" platform="ios" src="www/res/ios/Default-667h@2x.png" width="750" />
        <splash height="2208" platform="ios" src="www/res/ios/Default-Portrait-736h@3x.png" width="1242" />
        <splash height="1242" platform="ios" src="www/res/ios/Default-Landscape-736h@3x.png" width="2208" />
